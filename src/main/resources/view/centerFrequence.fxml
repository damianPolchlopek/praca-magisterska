<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.BorderPane?>

<?import javafx.scene.chart.CategoryAxis?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="600.0" stylesheets="@/css/centerFrequence.css"
            xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.polchlopek.praca.magisterska.controller.FrequenceGraph">
   <center>
       <BarChart title="Frequence Graph" BorderPane.alignment="CENTER" fx:id="barChartFrequence" >
           <xAxis>
               <CategoryAxis side="BOTTOM" fx:id="xAxis"/>
           </xAxis>
           <yAxis>
               <NumberAxis side="LEFT" fx:id="yAxis"/>
           </yAxis>
       </BarChart>
   </center>
</BorderPane>
